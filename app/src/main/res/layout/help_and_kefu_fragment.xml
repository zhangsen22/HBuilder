<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ptr="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal">

    <include layout="@layout/title"/>

    <include layout="@layout/line_10_f9"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="vertical">

    <LinearLayout
        android:id="@+id/ll_jiaocheng"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y80"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/y40"
        android:paddingRight="@dimen/y40"
        android:gravity="center_vertical"
        android:background="@color/common_white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="教程攻略"
            android:textSize="13sp"
            android:textColor="@color/color_666666"
            android:gravity="center"
            android:layout_marginLeft="@dimen/y30"/>

        <View
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"/>

        <ImageView
            android:layout_width="@dimen/y12"
            android:layout_height="@dimen/y20"
            android:src="@mipmap/y"/>

    </LinearLayout>

    <include layout="@layout/line_f9"/>

    <LinearLayout
        android:id="@+id/ll_alpayid"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y80"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/y40"
        android:paddingRight="@dimen/y40"
        android:gravity="center_vertical"
        android:background="@color/common_white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="如何获取支付宝id"
            android:textSize="13sp"
            android:textColor="@color/color_666666"
            android:gravity="center"
            android:layout_marginLeft="@dimen/y30"/>

        <View
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"/>

        <ImageView
            android:layout_width="@dimen/y12"
            android:layout_height="@dimen/y20"
            android:src="@mipmap/y"/>

    </LinearLayout>

    <include layout="@layout/line_f9"/>

    <LinearLayout
        android:id="@+id/ll_alishoukuanma"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y80"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/y40"
        android:paddingRight="@dimen/y40"
        android:gravity="center_vertical"
        android:background="@color/common_white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="支付宝商家码开通教程"
            android:textSize="13sp"
            android:textColor="@color/color_666666"
            android:gravity="center"
            android:layout_marginLeft="@dimen/y30"/>

        <View
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"/>

        <ImageView
            android:layout_width="@dimen/y12"
            android:layout_height="@dimen/y20"
            android:src="@mipmap/y"/>

    </LinearLayout>

    <include layout="@layout/line_f9"/>

    <LinearLayout
        android:id="@+id/ll_alipayedu"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y80"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/y40"
        android:paddingRight="@dimen/y40"
        android:gravity="center_vertical"
        android:background="@color/common_white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="支付宝提高账户安全级别以及单日额度"
            android:textSize="13sp"
            android:textColor="@color/color_666666"
            android:gravity="center"
            android:layout_marginLeft="@dimen/y30"/>

        <View
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"/>

        <ImageView
            android:layout_width="@dimen/y12"
            android:layout_height="@dimen/y20"
            android:src="@mipmap/y"/>

    </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/y108"
        android:orientation="horizontal"
        android:background="@color/common_white"
        android:gravity="center_vertical">

        <LinearLayout
            android:id="@+id/ll_kefu_webchat"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:background="@mipmap/cb"
            android:orientation="vertical"
            android:gravity="center">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/cs"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="客服微信"
                android:textSize="13sp"
                android:textColor="@color/common_white"
                android:gravity="center"
                android:layout_marginTop="@dimen/y8"/>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_kefu_tb"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:background="@mipmap/cc"
            android:orientation="vertical"
            android:gravity="center">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/ct"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="客服telegram"
                android:textSize="13sp"
                android:textColor="@color/common_white"
                android:gravity="center"
                android:layout_marginTop="@dimen/y8"/>

        </LinearLayout>

    </LinearLayout>

</LinearLayout>

